<%= form_for @plan do |f| %>
  <h2>
  </h2>
  <%= f.text_field :unit_name, placeholder: 'Name for metered usage unit' %>
  <%= f.text_field :included_units, placeholder: 'Included metered usage units' %>
  <%= f.text_field :overage_price, placeholder: 'Price per additional unit (cents)' %>
  <%= f.text_field :unavailable, hint: 'Will remove from user facing list' %>
  <%= f.text_field :description, placeholder: 'HAML will be interpreted' %>
  <div class="form-actions">
    <%= f.submit 'Save', class: 'btn btn-primary' %>
  </div>
<% end %>
